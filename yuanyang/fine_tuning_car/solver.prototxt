net: "yuanyang/fine_tuning_car/train_val.prototxt"
test_iter: 100
test_interval: 500
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
stepsize: 20000 #should be smaller than 20000
display: 20
max_iter: 100000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "yuanyang/fine_tuning_car/car_fine_tuning"
# uncomment the following to default to CPU mode solving
# solver_mode: GPU
