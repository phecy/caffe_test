  name: "flatten1"
  type: "Flatten"
  bottom: "pool5"
  top: "flatten1"
}
layer {
  name: "dropout_flatten"
  type: "Dropout"
  bottom: "flatten1"
  top: "flatten1"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "fc1"
  type: "InnerProduct"
  bottom: "flatten1"
  top: "fc1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
}
layer {
    name: "slice1"
    type: "Slice"
    bottom: "fc1"
    top: "slice1_1"
    top: "slice1_2"
    slice_param{
        slice_dim: 1
    }
}
layer {
    name: "eltwise1"
    type: "Eltwise"
    bottom: "slice1_1"
    bottom: "slice1_2"
    top: "eltwise1"
    eltwise_param{
        operation: MAX
    }
}
layer {
    name : "dropoutf12"
    type: "Dropout"
    bottom: "eltwise1"
    top: "eltwise1"
    dropout_param{
        dropout_ratio: 0.5
    }
}

# plug in softmax signal
layer {
  name: "classfy1_new"
  type: "InnerProduct"
  bottom: "eltwise1"
  top: "classfy1_new"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 11085
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
}
layer {
	name: "loss1"
	type: "SoftmaxWithLoss"
	bottom: "classfy1_new"
	bottom: "label"
}
layer {
	name: "accuracy"
	type: "Accuracy"
	bottom: "classfy1_new"
	bottom: "label"
	top: "accuracy"
	include {
		phase: TEST
	}
}
