layer {
  name: "conv1_1"
  type: "Convolution"
  bottom: "data"
  top: "conv1_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    kernel_size: 3
    stride: 1
	pad: 1
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
}
layer {
    name: "conv1_1_p"
    type: "PReLU" 
    bottom: "conv1_1"
    top: "conv1_1_p"
    param {
        decay_mult: 0
    }
}

layer {
  name: "conv1_2"
