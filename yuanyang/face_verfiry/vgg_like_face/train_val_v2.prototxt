layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    mirror: true
    crop_size: 224
    scale: 0.00390625
  }
  data_param {
    source: "yuanyang/face_verfiry/train_lmdb/"
    batch_size: 64
    backend: LMDB
  }
}
layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  transform_param {
    mirror: false
    scale: 0.00390625
    crop_size: 224
  }
  data_param {
    source: "yuanyang/face_verfiry/test_lmdb/"
    batch_size: 10
    backend: LMDB
  }
}

